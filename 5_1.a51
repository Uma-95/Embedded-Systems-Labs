//blink LED after 1sec

ORG 0000H
SJMP START
ORG 0035H
//MODE 1 TIMER 1  
START:MOV R0,#15 //15X65.535 MILLISEC=1SEC
MOV TMOD,#10H 
AGAIN:MOV TH1,#00H
MOV TL1,#00H
CPL P1.0
//START TIMER
SETB TR1
CHECK: JNB TF1,CHECK
//CLR TF AND STOP TIMER
CLR TF1
DJNZ R0,AGAIN
CLR TR1
SJMP START
END
	